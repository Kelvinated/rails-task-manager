<h1>My tasks</h1>
<% @tasks.each do |task| %>
<h4><%= link_to task.title, task_path(task.id) %>
  <%= link_to "(delete)", task_path(task.id), method: :delete, data: { confirm: "Delete this item?"} %></h4>
<h5>Completed? <%= task.completed %></h5>
<% end %>

<%= link_to "Add a new task", tasks_new_path %>
